# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_REFERER=https://github.com/Leejames2305/LLM-Assisted-Path-Negotiation-on-Warehouse
OPENROUTER_TITLE=LLM Multi-Robot Navigation

# LLM Model Configuration
CENTRAL_LLM_MODEL=zai/glm-4.5-air:free
AGENT_LLM_MODEL=google/gemma-2-9b-it:free

# Advanced OpenRouter Features
# Provider Order (comma-separated list of preferred providers)
# Leave empty to use default provider selection
OPENROUTER_PROVIDER_ORDER=
# Example: OPENROUTER_PROVIDER_ORDER=OpenAI,Anthropic,Google

# Reasoning Configuration for supported models (o1-preview, o1-mini, etc.)
OPENROUTER_REASONING_ENABLED=true
OPENROUTER_REASONING_EXCLUDE=true

# Advanced Reasoning Detection
# Force all models to use reasoning mode (useful for testing)
FORCE_REASONING_MODE=false

# Explicitly specify reasoning models (comma-separated, overrides auto-detection)
REASONING_MODELS=
# Example: REASONING_MODELS=o1-preview,o1-mini,custom-reasoning-model

LOG_SIMULATION=true
